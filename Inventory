from Tkinter import *
import tkMessageBox
import Tkinter

def get_letters():
    import csv
    import fileinput
    import os
    win=Tk()
    inventorylist = "C:/Users/Rebecca/Desktop/Overland/Ordering Guide.csv"
    brands = csv.reader(open(inventorylist))
    alphab=["Menu"]
    for brand in brands:
        alpha = brand[0][0]
        if alpha not in alphab:
            alphab.append(alpha)
        print alphab
    for letter in alphab:        
        newButtons=Tkinter.Button(win,text=letter)
        newButtons.pack()
        win.title("Overland Inventory")

    mainloop()

get_letters()
