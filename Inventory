from Tkinter import *
import tkMessageBox
import Tkinter

def get_letters():
    import csv
    import fileinput
    import os
    win=Tk()
    inventorylist = "C:/Users/Rebecca/Desktop/Overland/Ordering Guide.csv"
    brands = csv.reader(open(inventorylist))
    for brand in brands:
        newButtons=Tkinter.Button(win,text=brand[0])
        newButtons.pack()
    mainloop()

get_letters()
